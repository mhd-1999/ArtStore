@model IEnumerable<Art>
@{
    ViewBag.Title = ViewBag.Name;
}

<div class="container" style="margin-top:100px">
    <h2>@ViewBag.Name</h2>
    <div class="cat-main">
        <li><a href="@Url.Action("AllProduct", "Art", new { area = "" })">All</a></li>
        @Html.Action("GetAllCategory", "Category")

    </div>
    <div class="row">
        @foreach (var item in Model)
        {
            @Html.Partial("_ArtOverview", item)
        }
    </div>
</div>
