@model IEnumerable<Art>
@{
    ViewBag.Title = "AllProduct";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container" style="margin-top:100px">
    <h2>AllProduct</h2>
    <div class="cat-main">@Html.Action("GetAllCategory", "Category")</div>
    <div class="row">
        @foreach (var item in Model)
        {
            @Html.Partial("_ArtOverview", item)
        }
    </div>
</div>
